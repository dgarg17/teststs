
<div class="rrProductsWrapper json">
    <div class="rrProductPlacementWrapper">
        <h3>{{message}}</h3>
        <div id="scrollable" class="scrollable">
            <div class="items">
                {{#items}}
                <div class="productWrapper">
                    <div id="horiz" class="rr-product-frame">
                        <a href={{linkURL}}>
                            <img src="{{imageURL}}" alt="{{name}}" class="productImage">
                        </a>
                        <a class="productDescription" href="{{linkURL}}">
                            {{name}}
                        </a>
                        <div class="reviewRatingsContainer">
                            {{#if Reviews}}
                            <div id="rr_rating_{{id}}_link">
                                <a href="{{linkURL}}expand%3DURV%23URV">
                                    <img src="" data-rating="{{rating}}" class="starImage" id="rr_rating_{{id}}">
                                    <span class="reviews">({{Reviews}})</span>
                                </a>
                            </div>
                            {{/if}}
                        </div>
                        <div id="price_{{id}}" class="price"></div>
                    </div>
                </div>
                {{/items}}
            </div>
        </div>
        <a href="#" class="nextProducts next browse right">Next</a>
        <a href="#" class="previousProducts prev browse left disabled">Previous</a>
    </div>
</div>