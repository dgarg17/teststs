<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    helpPath="en/cq/current/wcm/default_components.html#Text"
    title="Text"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab1
            jcr:primaryType="cq:Widget"
            anchor="100%"
            title="Text"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <text
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Text"
                    hideLabel="true"
                    name="./text"
                    xtype="richtext">
                    <rtePlugins jcr:primaryType="nt:unstructured">
                        <links jcr:primaryType="nt:unstructured">
                            <linkDialogConfig
                                jcr:primaryType="nt:unstructured"
                                height="{Long}376">
                                <linkAttributes jcr:primaryType="cq:WidgetCollection">
                                    <ensightenClickTracking
                                        jcr:primaryType="cq:Widget"
                                        collapsed="{Boolean}true"
                                        collapsible="{Boolean}true"
                                        inputValue="advanced"
                                        name="./linkdialog/ensightenLinkTrackingTab"
                                        title="Ensighten Click Tracking"
                                        xtype="dialogfieldset">
                                        <items jcr:primaryType="cq:WidgetCollection">
                                            <enable
                                                jcr:primaryType="nt:unstructured"
                                                attribute="adhocensightenenable"
                                                fieldLabel="Enable Ensighten Click Tracking"
                                                name="./linkdialog/ensightenLinkTrackingEnableTracking"
                                                xtype="checkbox">
                                                <listeners
                                                    jcr:primaryType="nt:unstructured"
                                                    check="function(component){var dlg=component.findParentByType('rtelinkdialog');dlg.enableEnsightenFields(component.checked);}"/>
                                            </enable>
                                            <eventType
                                                jcr:primaryType="cq:Widget"
                                                attribute="adhocensighteneventtype"
                                                defaultValue="standardClick"
                                                fieldLabel="Event Type"
                                                name="./linkdialog/ensightenEventType"
                                                type="select"
                                                xtype="selection">
                                                <options jcr:primaryType="cq:WidgetCollection">
                                                    <standardClick
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Standard Click (Default)"
                                                        value="standardClick"/>
                                                    <partnerClick
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Partner Click"
                                                        value="partnerClick"/>
                                                </options>
                                            </eventType>
                                            <elementTitle
                                                jcr:primaryType="cq:Widget"
                                                attribute="adhocensightenelementtitle"
                                                fieldLabel="Element Title"
                                                name="./linkdialog/ensightenElementTitle"
                                                xtype="textfield"/>
                                            <eventData
                                                jcr:primaryType="cq:Widget"
                                                attribute="adhocensighteneventdata"
                                                fieldLabel="Event Data"
                                                name="./linkdialog/ensightenEventData"
                                                xtype="textfield"/>
                                        </items>
                                    </ensightenClickTracking>
                                </linkAttributes>
                            </linkDialogConfig>
                        </links>
                        <styles
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <paraformat
                            jcr:primaryType="nt:unstructured"
                            features="*">
                            <formats jcr:primaryType="cq:WidgetCollection">
                                <para
                                    jcr:primaryType="nt:unstructured"
                                    description="paragraph"
                                    tag="p"/>
                                <h3
                                    jcr:primaryType="nt:unstructured"
                                    description="Heading 3"
                                    tag="h3"/>
                                <h4
                                    jcr:primaryType="nt:unstructured"
                                    description="Heading 4"
                                    tag="h4"/>
                                <h5
                                    jcr:primaryType="nt:unstructured"
                                    description="Heading 5"
                                    tag="h5"/>
                                <h6
                                    jcr:primaryType="nt:unstructured"
                                    description="Heading 6"
                                    tag="h6"/>
                            </formats>
                        </paraformat>
                        <format
                            jcr:primaryType="nt:unstructured"
                            features="[bold,italic]"/>
                        <colorpicker
                            jcr:primaryType="nt:unstructured"
                            colors="[4D4E53,CC0000,FFFFFF,8C8D8E,00A8E2,00AFAB,F8BF00,76BD22,F28B00,A45B96,FF828A]"
                            defaultColor="4D4E53"
                            features="*"/>
                        <misctools
                            jcr:primaryType="nt:unstructured"
                            features="[specialchars]">
                            <specialCharsConfig jcr:primaryType="nt:unstructured">
                                <chars jcr:primaryType="nt:unstructured">
                                    <registered
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;reg;"/>
                                    <leftsinglequote
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;lsquo;"/>
                                    <rightsinglequote
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;rsquo;"/>
                                    <leftdoublequote
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;ldquo;"/>
                                    <rightdoublequote
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;rdquo;"/>
                                    <trademark
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;trade;"/>
                                    <endash
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;ndash; "/>
                                    <emdash
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;mdash; "/>
                                    <pound
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;pound; "/>
                                    <copyright
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;copy; "/>
                                    <middledot
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;middot; "/>
                                    <rightanglequote
                                        jcr:primaryType="nt:unstructured"
                                        entity="&amp;raquo;"/>
                                </chars>
                            </specialCharsConfig>
                        </misctools>
                    </rtePlugins>
                    <externalStyleSheets
                        jcr:primaryType="nt:unstructured"
                        sheet1="/etc/designs/cdw-global/clientlibs/css/main.css"/>
                    <cssStyles
                        jcr:primaryType="nt:unstructured"
                        _x002d_highlight="Red Highlight(Only use for heading)"
                        disclaimer="Disclaimer"
                        lineheight_1_5="1.5 Lines"
                        lineheight_2="2 Lines"
                        lineheight_2_5="2.5 Lines"
                        lineheight_3="3 Lines"/>
                </text>
                <isRichTextFlag
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./textIsRich"
                    value="true"
                    xtype="hidden"/>
            </items>
        </tab1>
    </items>
</jcr:root>
