<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="400"
    stateful="false"
    width="550"
    xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <basic
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_basic.infinity.json"
                    xtype="cqinclude"/>
                <advanced
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_advanced.infinity.json"
                    xtype="cqinclude"/>
                <image
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_image.infinity.json"
                    xtype="cqinclude"/>
                <cloudservices
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_cloudservices.infinity.json"
                    xtype="cqinclude"/>
                <blueprint
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_blueprint.infinity.json"
                    xtype="cqinclude"/>
                <livecopy
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_livecopy.infinity.json"
                    xtype="cqinclude"/>
                <layout
                    jcr:primaryType="cq:Panel"
                    title="Layout Configuration">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <contentwidth
                            jcr:primaryType="cq:Widget"
                            defaultValue="full"
                            fieldLabel="Content Width"
                            name="./contentwidth"
                            type="select"
                            xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <full
                                    jcr:primaryType="nt:unstructured"
                                    text="Full Width(Default)"
                                    value="full"/>
                                <_x0039_60px
                                    jcr:primaryType="nt:unstructured"
                                    text="960px"
                                    value="layout-CaaS-960"/>
                                <_x0038_00px
                                    jcr:primaryType="nt:unstructured"
                                    text="800px"
                                    value="layout-CaaS-800"/>
                            </options>
                        </contentwidth>
                    </items>
                </layout>
            </items>
        </tabs>
    </items>
</jcr:root>
