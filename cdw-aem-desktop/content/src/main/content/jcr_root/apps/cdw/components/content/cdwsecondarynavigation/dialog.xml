<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    cls="tabpanelcss"
    helpPath="en/cq/current/wcm/default_components.html#Text"
    title="Secondary Navigation"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <linkItems
            jcr:primaryType="cq:Widget"
            addItemLabel="Add A Link"
            autoScroll="{Boolean}true"
            cls="multifieldcls"
            name="./links"
            title="Links"
            xtype="multifield">
            <fieldConfig
                jcr:primaryType="cq:Widget"
                baseCls="multifieldpanelbasecls"
                cls="cssmultifieldpanel"
                ctCls="multifieldpanelcollapsedctcls"
                xtype="multifieldpanel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <ctaText
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="CTA Text"
                        key="ctaText"
                        width="80%"
                        xtype="textfield"/>
                    <ctaURL
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        cls="csspathfield"
                        fieldLabel="CTA Link"
                        key="ctaLink"
                        width="80%"
                        xtype="pathfield"/>
                    <ctaTarget
                        jcr:primaryType="cq:Widget"
                        defaultValue="_self"
                        fieldDescription="CTA link opens in same window by default."
                        fieldLabel="CTA Target"
                        key="ctaTarget"
                        type="select"
                        width="80%"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <same
                                jcr:primaryType="nt:unstructured"
                                text="Same Window (Default)"
                                value="_self"/>
                            <new
                                jcr:primaryType="nt:unstructured"
                                text="New Window"
                                value="_blank"/>
                        </options>
                    </ctaTarget>
                    <isSecondary
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Secondary navigation items will display in the secondary link grouping."
                        fieldLabel="Secondary?"
						defaultValue="false"
						inputValue="true"
                        key="isSecondary"
                        type="checkbox"
                        width="50%"
                        xtype="selection">
					</isSecondary>
                </items>
            </fieldConfig>
        </linkItems>
		<global
            jcr:primaryType="cq:Widget"
            cls="panelcss"
            title="Global Configurations"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <backgroundColor
                    jcr:primaryType="cq:Widget"
                    colors="[D02C30,C20430,F28B00,009D4F,009483,007299,76236D,B42573,FE4A5d,D8292F,F5B324,3DAF2C,00A19C,007CBB,923A80,E01783,FF828A,E74C39,F8BF00,76BD22,00AFAB,00A8E2,A45B96,EA6CB0,FFB0C0,E57E3E,FFD200,A6CE39,63CCCA,00B3E3,B588B9,F6A7D7,EEC7CE,F2BD7D,FFE900,C3D600,9ADADA,54C9E8,CEADD0,E8BCD8,F7DBE0,F2D1A0,FFEA77,E2E768,B0E3E4,96D5EA,E8BFDC,EDD3E2,D1D0CE,BCBCBC,A8A8AA,898B8E,63656A,54565B,4D4D4F,CC0000,FFFFFF]"
                    fieldLabel="Background Color"
					defaultValue="4D4E53"
					editable="{Boolean}true"
					showHexValue="{Boolean}true"
                    name="./backgroundColor"
                    xtype="colorfield"/>
                <textColor
                    jcr:primaryType="cq:Widget"
                    colors="[FFFFFF,4D4E53]"
                    fieldLabel="Text Color"
                    name="./textColor"
                    xtype="colorfield"/>
                <logoLink
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Logo Link"
                    name="./logoLink"
                    xtype="pathfield"/>
				<logoOrientation
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Logo Orientation"
                    name="./logoOrientation"
					defaultValue="horizontal"
					type="select"
                    xtype="selection">
					<options jcr:primaryType="cq:WidgetCollection">
                        <horizontal
                            jcr:primaryType="nt:unstructured"
                            text="Horizontal (Default)"
                            value="horizontal"/>
                        <vertical
                            jcr:primaryType="nt:unstructured"
                            text="Vertical"
                            value="vertical"/>
                    </options>
				</logoOrientation>
				<logoBackground
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Logo Background Style"
                    name="./logoBackground"
					defaultValue="transparent"
					type="select"
                    xtype="selection">
					<options jcr:primaryType="cq:WidgetCollection">
                        <transparent
                            jcr:primaryType="nt:unstructured"
                            text="Transparent (Default)"
                            value="transparent"/>
                        <white
                            jcr:primaryType="nt:unstructured"
                            text="White"
                            value="white"/>
                    </options>
				</logoBackground>
				<logoOrientation
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Logo Orientation"
                    name="./logoOrientation"
					defaultValue="horizontal"
					type="select"
                    xtype="selection">
					<options jcr:primaryType="cq:WidgetCollection">
                        <horizontal
                            jcr:primaryType="nt:unstructured"
                            text="Horizontal (Default)"
                            value="horizontal"/>
                        <vertical
                            jcr:primaryType="nt:unstructured"
                            text="Vertical"
                            value="vertical"/>
                    </options>
				</logoOrientation>
				<logoBackground
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Logo Background Style"
                    name="./logoBackground"
					defaultValue="transparent"
					type="select"
                    xtype="selection">
					<options jcr:primaryType="cq:WidgetCollection">
                        <transparent
                            jcr:primaryType="nt:unstructured"
                            text="Transparent (Default)"
                            value="transparent"/>
                        <white
                            jcr:primaryType="nt:unstructured"
                            text="White"
                            value="white"/>
                    </options>
				</logoBackground>>
                <headline
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Showcase Headline"
                    name="./headline"
                    xtype="textfield"/>
                <secondaryLinksLabel
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Secondary Links Dropdown Label"
					value="More Links"
                    name="./secondaryLinksLabel"
					defaultValue="More Links"
                    xtype="textfield"/>
                <linkTitle
                        jcr:primaryType="cq:Widget"
                        fieldDescription="If defined, will populate the 'title' attribute of the 'a' element."
                        fieldLabel="Link Title"
                        name="./linkTitle"
                        xtype="textfield"/>

                <eventDataDetails
                        jcr:primaryType="cq:Widget"
                        path="/apps/cdw/components/content/cdwherobanner/dialog/items/items/tab1/items/eventdata.infinity.json"
                        xtype="cqinclude"/>
            </items>
        </global>
        <image
            jcr:primaryType="cq:Widget"
            allowUpload="{Boolean}false"
            cropParameter=""
            ddGroups="[s7media]"
            disableZoom="{Boolean}true"
            fileNameParameter="./fileName"
            fileReferenceParameter="./fileReference"
            mapParameter=""
            name="./file"
            requestSuffix=".img.png"
            rotateParameter=""
            title="Partner Logo Image (Optional)"
            xtype="s7html5smartimage"/>
    </items>
</jcr:root>
