<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="{Long}500"
    title="Rich Relevance Placement"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Widget"
                anchor="100%"
                title="Configuration"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <placementId
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        fieldLabel="Placement ID"
                        name="./placementId"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
